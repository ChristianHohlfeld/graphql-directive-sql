  "description": "Treat your SQL schema as a subset of your GraphQL Schema. Use GraphQL SDL as the lingua franca to define your data requirements.",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "format": "./node_modules/.bin/prettier --write 'src/**/*.{ts}'",
    "lint": "./node_modules/.bin/tslint --fix 'src/**/*.{ts}'",
    "precommit": "lint-staged"
  },
  "repository": "git@github.com:taylorgoolsby/graphql-to-sql.git",
  "author": "Taylor Goolsby <taylorgoolsby@users.noreply.github.com>",
  "license": "GPL-3.0",
  "engines": {
    "node": ">=8.11.4"
  },
  "peerDependencies": {
    "graphql": "^14.0.2",
    "graphql-tools": "^4.0.0"
  },
  "devDependencies": {
    "graphql": "^14.0.2",
    "graphql-tools": "^4.0.0",
    "husky": "^1.1.1",
    "lint-staged": "^7.3.0",
    "prettier": "^1.14.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.1"
  }
}
